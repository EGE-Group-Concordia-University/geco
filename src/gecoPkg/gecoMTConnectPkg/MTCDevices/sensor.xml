<MTConnectDevices>
  <Devices>
    <Device id="dht11_1" name="DHT11">
      <Description>DHT11 Sensor</Description>
      <Configuration>
	<SensorConfiguration>
	  <FirmwareVersion>2.34</FirmwareVersion>
	  <Channels>
	    <Channel number="1" name="Temp">
	      <Description>Temperature in Celcius</Description>
	      <CalibrationDate>2010-05-16</CalibrationDate>
	      <NextCalibrationDate>2011-05-16</NextCalibrationDate>
	      <CalibrationInitials>RW</CalibrationInitials>
	    </Channel>
	    <Channel number="2" name="Humi">
	      <Description>Relative humidity in Percent</Description>
	      <CalibrationDate>2010-05-16</CalibrationDate>
	      <CalibrationInitials>RW</CalibrationInitials>
	    </Channel>
	  </Channels>
	</SensorConfiguration>
      </Configuration>
      <DataItems>
	<DataItem category="SAMPLE" id="Temp" type="TEMPERATURE" units="CELCIUS"/>
	<DataItem category="SAMPLE" id="Humi" type="HUMIDITY_REALTIVE" units="PERCENT"/>
      </DataItems>
    </Device>
  </Devices>
</MTConnectDevices>
